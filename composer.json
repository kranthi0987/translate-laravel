{
    "name": "weglot/translate-laravel",
    "description": "Seamless integration of Weglot into your Laravel project",
    "homepage": "https://weglot.com/",
    "type": "laravel-package",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Leduc",
            "email": "baptiste@weglot.com",
            "homepage": "https://baptiste-leduc.now.sh/",
            "role": "Developer"
        },
        {
            "name": "Thomas Deneulin",
            "email": "thomas@weglot.com",
            "role": "Review"
        }
    ],
    "support": {
        "email": "support@weglot.com",
        "source": "https://github.com/weglot/translate-laravel",
        "docs": "https://developers.weglot.com/laravel"
    },
    "keywords": [
        "php",
        "weglot",
        "weglot-integration",
        "translate",
        "translation",
        "translator",
        "localization",
        "languages",
        "i18n",
        "laravel",
        "laravel5"
    ],
    "require": {
        "weglot/weglot-php": "^0.6",
        "cache/illuminate-adapter": "^0.2.0",
        "illuminate/filesystem": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "Weglot\\Translate\\": "src/"
        },
        "files" : [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Weglot\\Translate\\TranslateServiceProvider"
            ]
        }
    }
}
